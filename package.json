{
  "name": "telegram-server",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "build": "npx tsc -b",
    "clean": "[ -d dist ] && rm -rf dist",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node app.ts\"",
    "tsstart": "npx ts-node app.ts",
    "start": "node dist/app.js"
  },
  "keywords": ["telegram", "bot", "simple"],
  "author": "Daniel Garcia <dev@danigarcia.org>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/form-data": "^2.5.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
